{
  "name": "nextjs-seed",
  "version": "0.4.4-0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "pre-lint": "npm run pretty:ci",
    "lint": "next lint",
    "pretty": "prettier --write .",
    "pretty:ci": "prettier --check .",
    "packages:check": "npx ncu --format group",
    "packages:update": "npx ncu --format group --interactive",
    "packages:install": "npm i",
    "post:packages:install": "npm prune",
    "postinstall": "run-script-os",
    "postprepare": "npm prune",
    "postinstall:default": "husky install",
    "postinstall:macos": "husky install && chmod ug+x .husky/*",
    "pre:contracts:build": "npx hardhat clean",
    "contracts:build": "npx hardhat compile",
    "contracts:clean": "npx hardhat clean",
    "contracts:run-node": "npx hardhat node",
    "pre:contracts:test": "npm run contracts:build",
    "contracts:test": "npx hardhat test",
    "contracts:deploy": "npx hardhat run --network localhost ./scripts/deploy.ts",
    "contracts:local-deploy": "npx hardhat run --network localhost ./scripts/localDeploy.js",
    "clean:all": "run-s clean:starter clean:contracts",
    "clean:node": "npx rimraf ./node_modules",
    "postclean:node": "npm run packages:install",
    "clean:starter": "npx rimraf ./src/pages/examples ./src/pages/starters",
    "clean:ts": "npx rimraf ./tsconfig.json",
    "postclean:ts": "renamer --find tsconfig.json.no-contracts --replace tsconfig.json *",
    "clean:contract-files": "npx rimraf ./contracts ./scripts ./test ./artifacts ./typechain-types ./cache ./hardhat.config.ts",
    "preclean:contracts": "run-s clean:contract-files clean:ts",
    "clean:contracts": "npm uninstall typechain solidity-coverage hardhat-gas-reporter hardhat @typechain/hardhat @typechain/ethers-v5 @nomiclabs/hardhat-etherscan @nomiclabs/hardhat-waffle @nomiclabs/hardhat-ethers @nomicfoundation/hardhat-toolbox @nomicfoundation/hardhat-chai-matchers @nomicfoundation/hardhat-network-helpers --save-dev",
    "postclean:contracts": "npm prune",
    "setup:contracts": "npm i typechain solidity-coverage hardhat-gas-reporter hardhat @typechain/hardhat @typechain/ethers-v5 @nomiclabs/hardhat-etherscan @nomiclabs/hardhat-waffle @nomiclabs/hardhat-ethers @nomicfoundation/hardhat-toolbox @nomicfoundation/hardhat-chai-matchers @nomicfoundation/hardhat-network-helpers --save-dev"
  },
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.13",
    "@nomiclabs/hardhat-etherscan": "^3.1.2",
    "@types/react-dom": "^18.0.9",
    "buffer": "^6.0.3",
    "daisyui": "^2.40.1",
    "discord.js": "^14.6.0",
    "ethers": "^5.7.2",
    "events": "^3.3.0",
    "formik": "^2.2.9",
    "framer-motion": "^10.16.5",
    "lodash": "^4.17.21",
    "next": "^13.0.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.8.3",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-daisyui": "^2.4.6",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-scripts": "5.0.1",
    "react-sound": "^1.2.0",
    "react-toastify": "^9.1.1",
    "recharts": "2.1.16",
    "run-script-os": "^1.1.6",
    "sharp": "^0.31.2",
    "util": "^0.12.5",
    "wagmi": "^0.8.3",
    "web-vitals": "^3.0.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@heroicons/react": "^2.0.13",
    "@next/bundle-analyzer": "^13.0.3",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "@typechain/ethers-v5": "^10.1.1",
    "@typechain/hardhat": "^6.1.4",
    "@types/babel__core": "^7.1.20",
    "@types/babel__generator": "^7.6.4",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^9.1.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.13",
    "chai": "^4.3.7",
    "classnames": "^2.3.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-next": "^13.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "hardhat": "^2.12.2",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.2",
    "npm-check-updates": "^16.4.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "prettier": "2.7.1",
    "renamer": "^4.0.0",
    "rimraf": "^3.0.2",
    "solidity-coverage": "^0.8.2",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typechain": "^8.1.1",
    "typescript": "^4.8.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
